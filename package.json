    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "clean-webpack-plugin": "^2.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
